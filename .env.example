# Arabic Web Proxy Configuration
# إعدادات وكيل الويب العربي

# Server Settings
PORT=3000
NODE_ENV=development

# Security
SESSION_SECRET=your-super-secret-session-key-change-this-in-production
COOKIE_SECRET=your-cookie-secret-change-this-in-production

# Supabase Configuration
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_ANON_KEY=your-anon-key

# Admin Credentials
ADMIN_USERNAME=admin
ADMIN_PASSWORD=admin123

# CORS
ALLOWED_ORIGINS=http://localhost:3000,https://yourdomain.com

# Proxy Settings
PROXY_TIMEOUT=30000
MAX_RESPONSE_SIZE=52428800

# Rate Limiting
RATE_LIMIT_WINDOW=900000
RATE_LIMIT_MAX=100

# Session Settings
SESSION_MAX_AGE=86400000

# Blocked Resources (comma-separated)
BLOCKED_DOMAINS=malware.com,phishing.com
BLOCKED_IPS=

# Feature Toggles
ENABLE_WEBSOCKETS=true
ENABLE_MEDIA_STREAMING=true
ENABLE_JS_REWRITE=true
ENABLE_COOKIE_SYNC=true
CACHE_ENABLED=false

# Bandwidth Limits (bytes)
FREE_TIER_BANDWIDTH=104857600
PREMIUM_TIER_BANDWIDTH=10737418240

# Logging
LOG_LEVEL=info
